# ansible inventory generated by terraform
[lsf_master]
%{ for index, instance in lsf_master_nodes ~}
${instance.name} ansible_host=${instance.network[0].fixed_ip_v4}
%{ endfor ~}

[lsf_compute]
%{ for index, instance in lsf_compute_private_nodes ~}
${instance.name} ansible_host=${instance.network[0].fixed_ip_v4}
%{ endfor ~}
%{ for index, instance in lsf_compute_public_nodes ~}
${instance.name} ansible_host=${instance.network[0].fixed_ip_v4}
%{ endfor ~}
